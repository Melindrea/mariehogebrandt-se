<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{{ pkg.name }}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">
        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
        <!-- build:css({.tmp,app}) styles/site.min.css -->
        <link rel="stylesheet" href="styles/site.css">
        <!-- endbuild -->
        <script src="bower_components/jquery/jquery.js"></script>

        <script src="bower_components/golden-grid-system/GGS.js"></script><!-- remove in production -->

        <!-- build:js scripts/head.min.js -->
        <script src="bower_components/modernizr/modernizr.min.js"></script>
        <script src="bower_components/selectivizr/selectivizr.js"></script>
        <script src="bower_components/WP-MarkDown/js/prettify.js"></script>
        <!-- endbuild -->

        <!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
        <script>
        var _gaq=[['_setAccount','UA-XXXXX-X'],['_trackPageview']];
        (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
            g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
            s.parentNode.insertBefore(g,s)}(document,'script'));
        </script>
    </head>
    <body{{#if bodyClass}} {{bodyClass}}{{/if}}>
        {{> header}}
        {{> outdated-warning}}


        <div class="l-container{{#if class}} {{class}}{{/if}}" role="document">
            <main class="content" role="main">
                <article class="inner{{#if articleClass}} {{articleClass}}{{/if}}">
                    {{#markdown}}
                    {{> body}}
                    {{/markdown}}
                </article>
            </main>
            {{#is class "content-first"}}
            <aside class="sidebar widget-area" role="complementary">
                <div class="inner">
                    {{> sidebar }}
                </div>
            </aside>
            {{/is}}
        </div>


        {{> footer}}
        <!-- build:js scripts/main.min.js -->
        <script src="bower_components/WP-MarkDown/js/pagedown/Markdown.Converter.js"></script>
        <script src="bower_components/WP-MarkDown/js/pagedown/Markdown.Sanitizer.js"></script>
        <script src="bower_components/WP-MarkDown/js/pagedown/Markdown.Editor.js"></script>
        <script src="bower_components/WP-MarkDown/js/markdown.js"></script>
        <script src="bower_components/jquery-mousewheel/jquery.mousewheel.js"></script>
        <script src="bower_components/fancybox/source/jquery.fancybox.js"></script>
        <script src="bower_components/fancybox/source/helpers/jquery.fancybox-media.js"></script>
        <script src="scripts/main.js"></script>
        <script src="scripts/fancybox.js"></script>
        <script src="scripts/external-links.js"></script>
        <!-- endbuild -->
    </body>
</html>
